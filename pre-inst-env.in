#!/bin/sh

abs_top_srcdir="`cd "@abs_top_srcdir@" > /dev/null; pwd`"
abs_top_builddir="`cd "@abs_top_builddir@" > /dev/null; pwd`"

GUILE_LOAD_COMPILED_PATH="$abs_top_builddir/lisp${GUILE_LOAD_COMPILED_PATH:+:}$GUILE_LOAD_COMPILED_PATH"
GUILE_LOAD_PATH="$abs_top_builddir/lisp:$GUILE_LOAD_PATH"
export GUILE_LOAD_COMPILED_PATH GUILE_LOAD_PATH
LTDL_LIBRARY_PATH="$abs_top_builddir/.libs/"
export LTDL_LIBRARY_PATH
PATH="$abs_top_builddir/scripts:$abs_top_builddir:$PATH"
export PATH

exec "$@"
